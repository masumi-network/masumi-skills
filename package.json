{
  "name": "@masumi/openclaw-skills",
  "version": "1.0.0",
  "description": "Modular OpenClaw skills monorepo: Cardano Wallet, Masumi Payments, and Sokosumi Marketplace",
  "private": true,
  "workspaces": [
    "skills/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:cardano-wallet": "npm run build --workspace=skills/cardano-wallet",
    "build:masumi-payments": "npm run build --workspace=skills/masumi-payments",
    "build:sokosumi-marketplace": "npm run build --workspace=skills/sokosumi-marketplace",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "keywords": [
    "masumi",
    "openclaw",
    "blockchain",
    "payments",
    "ai-agents",
    "cardano",
    "web3",
    "cryptocurrency",
    "sokosumi",
    "marketplace"
  ],
  "author": "Masumi Network",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masumi-network/masumi-openclaw-skill.git"
  }
}
